						//上传文件
						url = url.replace(new RegExp("___", "g"),"/");
        				$("#"+spanid).html(html);
        				$("#"+inputid).attr("value",url);	
						
						$("#"+id).hide();
						
            		}else{
            			wxcalert('上传失败','e');
            		}
