
	<div class="g_block_2 g_block_7 mb20">
		<table class="mb10" cellpadding="0" cellspacing="0" border="0">
			<tr>
						<th>所在省市</th>
						<th>业务方向</th>
						<th>公司名称(品牌)</th>
						<th>负责人/电话</th>
						<th>计调/电话</th>
						<th>运维费状态</th>
						<th>一级代理返利</th>
			</tr>
			#foreach($item in $dataList)
				<tr>
                  	  <td>#vregion($!item.cpyProvince) #vregion($!item.cpyCity) #vregion($!item.cpyCounty)</td>
